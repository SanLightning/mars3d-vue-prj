{
  "name": "mars3d-vue-project",
  "version": "3.6.0",
  "description": "在Vue3技术栈下的Mars3D平台 基础项目",
  "scripts": {
    "dev": "vite",
    "serve": "npm run clean-cache && vite --host",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "preview": "vite preview",
    "build": "npm run lint && vite build",
    "build2": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "serve:dist": "http-server ./dist",
    "lint": "vue-tsc --noEmit --skipLibCheck && npm run eslint",
    "eslint": "eslint ./src/**/*.{js,vue,ts} --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@icon-park/svg": "^1.4.2",
    "@mars3d/heatmap.js": "^2.0.7",
    "@turf/turf": "^6.5.0",
    "ant-design-vue": "4.0.4",
    "axios": "^1.5.1",
    "core-js": "^3.33.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-gl": "^2.0.9",
    "kml-geojson": "^1.2.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mapv": "^2.0.62",
    "mars3d": "~3.6.0",
    "mars3d-cesium": "~1.110.0",
    "mars3d-echarts": "~3.6.0",
    "mars3d-heatmap": "~3.6.0",
    "mars3d-mapv": "~3.6.0",
    "mars3d-space": "~3.6.0",
    "nprogress": "^0.2.0",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-color-kit": "^1.0.6",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "consola": "^3.2.3",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "fs-extra": "^11.1.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "serve-static": "^1.15.0",
    "standard": "^17.1.0",
    "terser": "^5.22.0",
    "typescript": "5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mars3d": "^3.0.3",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^1.8.19"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars3d-vue-project.git"
  },
  "bugs": {
    "url": "https://github.com/marsgis/mars3d-vue-project/issues",
    "email": "wh@marsgis.cn"
  },
  "keywords": [
    "marsgis",
    "mars3d",
    "cesium",
    "vue",
    "vue3",
    "gis",
    "webgis"
  ],
  "author": "火星科技",
  "license": "Apache-2.0",
  "homepage": "http://mars3d.cn",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run eslint"
    ]
  }
}
